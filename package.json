{
  "name": "vscode-svgts",
  "displayName": "vscode-svgts",
  "description": "Display svgts file in VSCode.",
  "version": "0.1.0",
  "publisher": "xperiments",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "svg",
    "svgts",
    "viewer"
  ],
  "icon": "vscode-svgts.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xperiments/vscode-svgts.git"
  },
  "bugs": {
    "url": "https://github.com/xperiments/vscode-svgts/issues"
  },
  "activationEvents": [
    "onLanguage:svgts",
    "onCommand:extension.svgts-preview",
    "onCommand:extension.svgts-generate-from-dir"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "svgts",
        "aliases": [
          "svgts viewer",
          "svgts"
        ],
        "extensions": [
          ".svgts"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "svgts",
        "scopeName": "text.plain, text.plain.svgts",
        "path": "./syntaxes/svgts.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "extension.svgts-preview",
        "title": "View svgts"
      },
      {
        "command": "extension.svgts-generate-from-dir",
        "title": "Create svgts module"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == svgts",
          "command": "extension.svgts-preview",
          "group": "navigation"
        },
        {
          "command": "extension.svgts-generate-from-dir",
          "group": "svgts",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "package": "npm run client && vsce package",
    "client": "./scripts/package-client.sh"
  },
  "dependencies": {
    "vscode": "^1.1.21"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "tslint": "^3.15.1",
    "typescript": "^2.9.1",
    "vsce": "^1.42.0"
  },
  "homepage": "https://github.com/tomoki1207/vscode-pdfviewer#readme",
  "author": "Pedro Casaubon",
  "license": "MIT"
}
